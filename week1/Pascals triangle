class Solution {
    public List<List<Integer>> generate(int numRows) {
        List<List<Integer>> res= new ArrayList<>();
        for(int i=0;i<numRows;i++){
            List<Integer> inte= new ArrayList<>();
            inte.add(1);
            for(int j=1;j<i;j++){
                inte.add(res.get(i-1).get(j-1)+res.get(i-1).get(j));
            }
            if(i>0){
                inte.add(1);
            }
            res.add(inte);
        }
        return res;
    }
}
